<div class="successWrapper">
  {{#each data}}
  <div class="confirmation">
    You have sent {{currencySymbol}}{{amount}} to {{email}}!
  </div>
  {{/each}}
  <br />
  <i class="fa fa-check fa-4x"></i>

</div>
<div class="buttonWrapper">
  <a href="send" class="sendMoreMoneyLink">
    <button class="btn btn-group sendMoreMoneyBtn">Send Money</button>
  </a>
  <a href="transactions" class="transactionHistoryLink">
    <button class="btn btn-group transactionHistoryBtn">Transaction History</button>
  </a>
</div>
